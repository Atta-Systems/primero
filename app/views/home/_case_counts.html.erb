<div class="panel">
  <div class="panel_header">
    <h4><%= t("dashboard.case_stats") %></h4>
  </div>
  <div class="panel_content no-padding panel_main">
    <table cellspacing="0" cellpadding="0" class="table-floating-header">
      <tbody>
        <tr>
          <td>You have <%= link_to total_cases, '' %> total cases and <%= link_to new_cases, '' %> new cases </td>
        </tr>
        <tr>
          <td>You have 10 high risk cases and 2 new high risk cases </td>
        </tr>
        <tr>
          <td>You have 10 medium risk cases and 2 new medium risk cases </td>
        </tr>
        <tr>
          <td>You have 10 low risk cases and 2 new low risk cases </td>
        </tr>
        <tr>
          <td>You have 10 total referrals and 2 new referrals </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
