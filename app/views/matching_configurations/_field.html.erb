<% forms_options.each do |form_name, form_key| %>
  <div class='row <%= row_selector %><%= ' hide_element' unless forms.include?(form_key) %>' id='<%= form_key %>' >
    <% field_options, form_name = matching_configuration.get_form_field_options(form_type, form_key) %>
    <div class='row field_form_label' >
      <%= f.label field_selector, form_name, :class => "key inline" %>
    </div>
    <%= f.select field_selector,
          options_for_select(field_options,
                            matching_configuration.get_form_matchable_fields(form_type, form_key)),
          { include_hidden: false },
          { multiple: true,
            'data-placeholder': t('fields.select_box_empty_item'),
            class:'chosen-select',
            id: "#{select_option_id + form_key}",
            name: "matching_configuration[#{field_selector}][#{form_key}][]"
          }
    %>
  </div>
<% end %>
