<% if formObject[form_id].present? %>
  <div class="uploaded-documents">
    <% formObject[form_id].each do |document| %>
      <div class="row">
        <div class="medium-4 columns">
          <label class="key"><%= I18n.t("fields.#{field.name}") %></label>
        </div>
        <div class="medium-8 columns">
          <% document_url = send("#{formObject.class.name.underscore.downcase}_document_url", formObject.id, document["attachment_key"]) %>
          <%= link_to document["file_name"], document_url, :class => "document" %>
        </div>
      </div>
      <div class="row">
        <div class="medium-4 columns">
          <label class="key document-description">Document Name</label>
        </div>
        <div class="medium-8 columns">
          <% value = document["document_description"] || "" %>
          <span class="document-description"><%= value %></span>
        </div>
      </div>
      <div class="row">
        <div class="medium-4 columns">
          <label class="key date">Document Date</label>
        </div>
        <div class="medium-8 columns">
          <% value = document["date"] || "" %>
          <span class="date"><%= value %></span>
        </div>
      </div>
      <div class="row">
        <div class="medium-4 columns">
          <label class="key comments">Comments</label>
        </div>
        <div class="medium-8 columns">
          <% value = document["comments"] || "" %>
          <span class="comments"><%= value %></span>
        </div>
      </div>
    <% end %>
  </div>
<% end %>