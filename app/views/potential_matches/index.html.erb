<div class="row collapse">
  <div class="large-9 small-9 columns">
    <div class="clearfix"></div>
    <table style="width: 100%;" class="record_list_view dataTable no-footer collapsed dtr-inline" cellpadding="0" cellspacing="0">
      <thead>
      <tr>
        <th>child_id</th>
        <th>tracing_request_id</th>
        <th>score</th>
        <th>status</th>
      </tr>
      </thead>
      <tbody>
      <% @potential_matches.each do |match| %>
          <tr>
            <td><%= match.child_id %></td>
            <td><%= match.tracing_request_id %></td>
            <td><%= match.score %></td>
            <td><%= match.status %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
    <div class="table_spacer"></div>
  </div>
</div>