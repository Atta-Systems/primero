<% stats.each do |k, v| %>
<div>
  <%= I18n.translate('dashboard.manager_transfers_totals',
      case_count(
        {  count: v[:total_cases],
           stat_type: k,
           filter: index_filters(['child_status=Open', 'transfer_status=In Progress' ,'record_state=true'])
        }, nil, model)).html_safe %>
</div>
<% end %>