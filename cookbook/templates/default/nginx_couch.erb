server {
  listen 6984 ssl;
  ssl_certificate <%= @ssl_cert_path %>;
  ssl_certificate_key <%= @ssl_key_path %>;
  client_max_body_size 50M;

  proxy_buffering off;

  access_log <%= @log_dir %>/nginx_server.log;
  error_log <%= @log_dir %>/nginx_error.log error;

  location / {
    proxy_pass http://localhost:5984;
  }
}
